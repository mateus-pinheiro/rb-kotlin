<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        app:layout_behavior="@string/appbar_scrolling_view_behavior"
        tools:showIn="@layout/activity_search_barcode_traceable"
        tools:context=".ui.SearchBarcodeTraceableActivity">

    <ScrollView
            android:layout_width="match_parent"
            android:layout_height="match_parent">
        <android.support.constraint.ConstraintLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent">

            <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/tvCreationDate" app:layout_constraintStart_toStartOf="@+id/textView"
                    android:layout_marginTop="8dp"
                    app:layout_constraintTop_toBottomOf="@+id/textView" android:textColor="@android:color/black"
                    app:layout_constraintEnd_toEndOf="@+id/textView"/>
            <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/tvSerialNumber" app:layout_constraintStart_toStartOf="@+id/textView28"
                    android:layout_marginTop="5dp"
                    app:layout_constraintTop_toBottomOf="@+id/textView28" android:textColor="@android:color/black"
            />
            <TextView
                    android:text="Dados do Item"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:id="@+id/largeLabel2"
                    android:visibility="gone"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintEnd_toEndOf="parent" app:layout_constraintHorizontal_bias="0.501"
                    android:textStyle="bold" android:textSize="18sp" android:layout_marginTop="100dp"
                    app:layout_constraintTop_toBottomOf="@+id/textView31" android:textAlignment="center"/>
            <TextView
                    android:hint=""
                    android:textColorHint="@android:color/black"
                    android:gravity="center"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:id="@+id/tvBarcode" android:layout_marginTop="8dp"
                    app:layout_constraintTop_toBottomOf="@+id/largeLabel" app:layout_constraintStart_toStartOf="parent"
                    android:layout_marginStart="14dp" app:layout_constraintEnd_toEndOf="parent"
                    android:layout_marginEnd="14dp" android:textColor="@android:color/black"/>
            <TextView
                    android:text="Entidade Responsável"

                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/textView23" app:layout_constraintStart_toStartOf="@+id/button"
                    android:layout_marginTop="16dp"
                    app:layout_constraintTop_toBottomOf="@+id/button"/>
            <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/tvEntityOwner" app:layout_constraintStart_toStartOf="@+id/textView25"
                    android:layout_marginTop="8dp"
                    app:layout_constraintTop_toBottomOf="@+id/textView25" android:textColor="@android:color/black"
            />
            <TextView
                    android:text="Fabricação"
                    android:visibility="gone"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/textView" app:layout_constraintStart_toStartOf="@+id/largeLabel2"
                    android:layout_marginTop="16dp"
                    app:layout_constraintTop_toBottomOf="@+id/largeLabel2"
                    app:layout_constraintEnd_toEndOf="@+id/largeLabel2"/>
            <TextView
                    android:text="Tipo"
                    android:visibility="gone"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/tvItemTypeLabel"
                    app:layout_constraintEnd_toEndOf="@+id/button" android:layout_marginTop="16dp"
                    app:layout_constraintTop_toBottomOf="@+id/button"/>
            <TextView
                    android:text="Identificador"
                    android:visibility="gone"
                    android:layout_width="90dp"
                    android:layout_height="wrap_content"
                    android:id="@+id/textView30" app:layout_constraintStart_toStartOf="parent"
                    android:layout_marginTop="90dp"
                    app:layout_constraintTop_toBottomOf="@+id/tvCreationDate"
                    app:layout_constraintEnd_toEndOf="parent" android:textAlignment="center"/>
            <TextView
                    android:text="Apresentação"
                    android:visibility="gone"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/textView29"
                    android:layout_marginTop="16dp"
                    app:layout_constraintTop_toBottomOf="@+id/tvCreationDate" app:layout_constraintEnd_toEndOf="parent"
                    android:layout_marginEnd="16dp"/>
            <TextView
                    android:text="Lote"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/textView27"
                    android:visibility="gone"
                    android:layout_marginTop="16dp"
                    app:layout_constraintTop_toBottomOf="@+id/largeLabel2" app:layout_constraintEnd_toEndOf="parent"
                    android:layout_marginEnd="16dp"/>
            <TextView
                    android:text="Número de Série"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/textView28" app:layout_constraintStart_toStartOf="parent"
                    android:layout_marginTop="16dp"
                    android:visibility="gone"
                    app:layout_constraintTop_toBottomOf="@+id/tvCreationDate" android:layout_marginStart="16dp"/>
            <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/tvBatchNumber"
                    android:textColor="@android:color/black"
                    app:layout_constraintEnd_toEndOf="@+id/textView27" android:layout_marginTop="8dp"
                    app:layout_constraintTop_toBottomOf="@+id/textView27"/>
            <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/tvGtinNumber"
                    android:layout_marginTop="5dp"
                    app:layout_constraintTop_toBottomOf="@+id/textView29" android:textColor="@android:color/black"
                    app:layout_constraintEnd_toEndOf="@+id/textView29"/>
            <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/tvAnvisaNumber" app:layout_constraintStart_toStartOf="@+id/textView30"
                    android:layout_marginTop="5dp"
                    app:layout_constraintTop_toBottomOf="@+id/textView30" android:textColor="@android:color/black"
                    app:layout_constraintEnd_toEndOf="@+id/textView30"
                    android:layout_marginBottom="24dp" app:layout_constraintBottom_toBottomOf="parent"/>
            <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/tvProduct" app:layout_constraintStart_toStartOf="@+id/textView26"
                    android:layout_marginTop="8dp"
                    app:layout_constraintTop_toBottomOf="@+id/textView26" android:textColor="@android:color/black"
            />
            <TextView
                    android:text="Item Escaneado:"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/largeLabel" app:layout_constraintTop_toTopOf="parent"
                    app:layout_constraintStart_toStartOf="parent" android:layout_marginTop="115dp"
                    app:layout_constraintEnd_toEndOf="parent"/>
            <View
                    android:text="Button"
                    android:layout_width="0dp"
                    android:layout_height="1dp"
                    android:background="@color/gray"
                    android:id="@+id/button" android:layout_marginTop="8dp"
                    app:layout_constraintTop_toBottomOf="@+id/tvBarcode" app:layout_constraintStart_toStartOf="parent"
                    android:layout_marginStart="16dp" app:layout_constraintEnd_toEndOf="parent"
                    android:layout_marginEnd="16dp"/>
            <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/tvItemType"
                    android:layout_marginTop="8dp"
                    app:layout_constraintTop_toBottomOf="@+id/tvItemTypeLabel" android:textColor="@android:color/black"
                    app:layout_constraintEnd_toEndOf="@+id/tvItemTypeLabel"/>
            <TextView
                    android:text="Entidade com Posse"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"

                    android:id="@+id/textView25" app:layout_constraintStart_toStartOf="@+id/tvEntityResponsible"
                    android:layout_marginTop="16dp"
                    app:layout_constraintTop_toBottomOf="@+id/tvEntityResponsible"/>
            <TextView
                    android:text="Produto"
                    android:visibility="gone"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/textView26" app:layout_constraintStart_toStartOf="parent"
                    android:layout_marginTop="16dp"
                    app:layout_constraintTop_toBottomOf="@+id/largeLabel2" android:layout_marginStart="16dp"/>
            <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/tvEntityResponsible" app:layout_constraintStart_toStartOf="@+id/textView23"
                    android:layout_marginTop="8dp"
                    app:layout_constraintTop_toBottomOf="@+id/textView23" android:textColor="@android:color/black"/>
            <TextView
                    android:text="Último Evento"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:id="@+id/textView31"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    android:textStyle="bold" android:textSize="18sp" app:layout_constraintHorizontal_bias="0.516"
                    app:layout_constraintTop_toBottomOf="@+id/button" android:layout_marginTop="170dp"
                    android:textAlignment="center"/>
            <TextView
                    android:text="Último Evento"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/textView32"
                    app:layout_constraintTop_toBottomOf="@+id/textView31" android:layout_marginTop="16dp"
                    app:layout_constraintStart_toStartOf="parent" android:layout_marginStart="16dp"/>
            <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/tvEventId" android:layout_marginTop="8dp"
                    app:layout_constraintTop_toBottomOf="@+id/textView32"
                    app:layout_constraintStart_toStartOf="@+id/textView32" android:textColor="@android:color/black"/>
        </android.support.constraint.ConstraintLayout>
    </ScrollView>
</android.support.constraint.ConstraintLayout>